{
	"name": "Card Hover",
	"version": "1.1",
	"author": [
		"PhiLtheFisH",
		"[https://fo-nttax.de Alex Winkler]"
	],
	"url": "https://liquipedia.net",
	"descriptionmsg": "cardhover-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.25.0"
	},
	"MessagesDirs": {
		"CardHover": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"Liquipedia\\CardHover\\Hooks": "Hooks.php"
	},
	"Hooks": {
		"ParserBeforeInternalParse": [
			"Liquipedia\\CardHover\\Hooks::onParserBeforeInternalParse"
		],
		"BeforePageDisplay": [
			"Liquipedia\\CardHover\\Hooks::onBeforePageDisplay"
		]
	},
	"ResourceModules": {
		"ext.cardHover": {
			"scripts": [
				"CardHover.js"
			],
			"styles": [
				"CardHover.css"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "CardHover"
	},
	"manifest_version": 1
}